[loggers]
keys=root,main,events,lotame,user

[handlers]
keys=TRotatingFileHandler,ERotatingFileHandler,LRotatingFileHandler,ARotatingFileHandler

[formatters]
keys=simpleFormatter

[logger_root]
level=DEBUG
handlers=TRotatingFileHandler

[logger_main]
level=DEBUG
handlers=TRotatingFileHandler
qualname=TRotatingFileHandler
propagate=0

[logger_events]
level=DEBUG
handlers=ERotatingFileHandler
qualname=events
propagate=0

[logger_lotame]
level=DEBUG
handlers=LRotatingFileHandler
qualname=lotame
propagate=0

[logger_user]
level=DEBUG
handlers=ARotatingFileHandler
qualname=user
propagate=0

[handler_TRotatingFileHandler]
class=handlers.TimedRotatingFileHandler
level=DEBUG
formatter=simpleFormatter
args=('/mnt/logs/djoint/dovetail_joint.log','M',20,10,)

[handler_ERotatingFileHandler]
class=handlers.TimedRotatingFileHandler
level=DEBUG
formatter=simpleFormatter
args=('/mnt/logs/djoint/dovetail_joint_events.log','M',20,10,)

[handler_LRotatingFileHandler]
class=handlers.TimedRotatingFileHandler
level=DEBUG
formatter=simpleFormatter
args=('/mnt/logs/djoint/lotame.log','M',20,10,)

[handler_ARotatingFileHandler]
class=handlers.TimedRotatingFileHandler
level=DEBUG
formatter=simpleFormatter
args=('/mnt/logs/djoint/app.log','W0',20,10,)

[formatter_simpleFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s